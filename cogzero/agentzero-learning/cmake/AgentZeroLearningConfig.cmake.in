# AgentZeroLearningConfig.cmake.in - CMake package configuration
# Part of AZ-LEARN-003: MOSES Policy Optimization Integration

@PACKAGE_INIT@

# Define version
set(AGENTZERO_LEARNING_VERSION "@PROJECT_VERSION@")

# Import targets
include("${CMAKE_CURRENT_LIST_DIR}/AgentZeroLearningTargets.cmake")

# Dependencies
find_dependency(PkgConfig REQUIRED)
pkg_check_modules(COGUTIL REQUIRED cogutil)
pkg_check_modules(ATOMSPACE REQUIRED atomspace)
pkg_check_modules(MOSES REQUIRED moses)
pkg_check_modules(ASMOSES REQUIRED asmoses)

# Optional dependencies
pkg_check_modules(LEARN learn)
pkg_check_modules(MINER miner)
pkg_check_modules(URE ure)

# Set variables for compatibility
set(AGENTZERO_LEARNING_FOUND TRUE)
set(AGENTZERO_LEARNING_LIBRARIES AgentZero::agentzero-learning)
set(AGENTZERO_LEARNING_INCLUDE_DIRS "${PACKAGE_PREFIX_DIR}/include")

check_required_components(AgentZeroLearning)