@PACKAGE_INIT@

include("@CMAKE_INSTALL_PREFIX@/lib/cmake/AgentZeroCpp/AgentZeroCppTargets.cmake")

set(AGENTZERO_CORE_LIBRARY agentzero-core)
set(AGENTZERO_LIBRARIES agentzero-core)

# Agent-Zero data directory for configuration files and resources
set(AGENTZERO_DATA_DIR "@CMAKE_INSTALL_PREFIX@/share/opencog/agents")
set(AGENTZERO_INCLUDE_DIR "@CMAKE_INSTALL_PREFIX@/include/")
set(AGENTZERO_VERSION "@PROJECT_VERSION@")
set(AGENTZERO_FOUND 1)

# Copy the results to the output variables.
IF (AGENTZERO_CORE_LIBRARY)
    SET(AGENTZERO_FOUND 1)
    SET(AGENTZERO_LIBRARIES ${AGENTZERO_CORE_LIBRARY})
    MESSAGE(STATUS "Found Agent-Zero library: ${AGENTZERO_LIBRARIES}")
ELSE (AGENTZERO_CORE_LIBRARY)
    SET(AGENTZERO_FOUND 0)
    SET(AGENTZERO_LIBRARIES)
ENDIF (AGENTZERO_CORE_LIBRARY)

MARK_AS_ADVANCED(
    AGENTZERO_LIBRARIES
    AGENTZERO_INCLUDE_DIR
    AGENTZERO_DATA_DIR
)